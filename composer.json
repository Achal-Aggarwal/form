{
    "name": "joomla/form",
    "type": "joomla-package",
    "description": "Joomla Form Package",
    "keywords": ["joomla", "framework", "form"],
    "homepage": "https://github.com/joomla/joomla-framework-form",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/language": "1.0-beta3",
        "joomla/string": "1.0-beta3",
        "joomla/uri": "1.0-beta3",
        "joomla/filter": "1.0-beta3",
        "joomla/filesystem": "1.0-beta3",
        "joomla/registry": "1.0-beta3",
        "joomla/utilities": "1.0-beta3"
    },
    "suggest": {
        "joomla/database": "Install joomla/database if you would like to use the DatabaseConnection field type."
    },
    "require-dev": {
        "joomla/test": "1.0-beta3"
    },
    "target-dir": "Joomla/Form",
    "autoload": {
        "psr-0": {
            "Joomla\\Form": ""
        }
    }
}
